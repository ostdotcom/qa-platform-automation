                  "pattern": ""
                },
                "type": {
                  "$id": "#/properties/data/properties/transaction/properties/meta_property/properties/type",
                  "type": "string",
                  "title": "The Type Schema",
                  "default": "",
                  "examples": [
                    "company_to_user"
                  ],
                  "pattern": ""
                },
                "details": {
                  "$id": "#/properties/data/properties/transaction/properties/meta_property/properties/details",
                  "type": "string",
                  "title": "The Details Schema",
                  "default": "",
                  "examples": [
                    "details of meta"
                  ],
                  "pattern": ""
                }
              }
            },
            "transfers": {
              "$id": "#/properties/data/properties/transaction/properties/transfers",
              "type": "array",
              "title": "The Transfers Schema",
              "items": {
                "$id": "#/properties/data/properties/transaction/properties/transfers/items",
                "type": "object",
                "title": "The Items Schema",
                "required": [
                  "from",
                  "from_user_id",
                  "to",
                  "to_user_id",
                  "kind"
