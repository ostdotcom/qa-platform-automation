              "title": "The Transaction_hash Schema",
              "default": null,
              "examples": [
                null
              ]
            },
            "from": {
              "$id": "#/properties/data/properties/transaction/properties/from",
              "type": "null",
              "title": "The From Schema",
              "default": null,
              "examples": [
                null
              ]
            },
            "to": {
              "$id": "#/properties/data/properties/transaction/properties/to",
              "type": "string",
              "title": "The To Schema",
              "default": "",
              "examples": [
                "0xfae609af29acc68a291b8e63a442c93c2adc05d8"
              ],
              "pattern": ""
            },
            "nonce": {
              "$id": "#/properties/data/properties/transaction/properties/nonce",
              "type": "null",
              "title": "The Nonce Schema",
              "default": null,
              "examples": [
                null
              ]
            },
            "value": {
              "$id": "#/properties/data/properties/transaction/properties/value",
              "type": "null",
              "title": "The Value Schema",
              "default": null,
              "examples": [
                null
              ]
            },
            "gas_price": {
              "$id": "#/properties/data/properties/transaction/properties/gas_price",
              "type": "string",
              "title": "The Gas_price Schema",
              "default": "",
              "examples": [
                "1000000000"
              ],
              "pattern": ""
            },
            "gas_used": {
              "$id": "#/properties/data/properties/transaction/properties/gas_used",
              "type": "null",
              "title": "The Gas_used Schema",
              "default": null,
              "examples": [
                null
              ]
            },
            "transaction_fee": {
              "$id": "#/properties/data/properties/transaction/properties/transaction_fee",
              "type": "null",
              "title": "The Transaction_fee Schema",
              "default": null,
              "examples": [
                null
              ]
            },
            "block_confirmation": {
              "$id": "#/properties/data/properties/transaction/properties/block_confirmation",
              "type": "null",
              "title": "The Block_confirmation Schema",
              "default": null,
              "examples": [
                null
              ]
            },
