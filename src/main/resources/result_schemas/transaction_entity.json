                  "kind": {
                    "$id": "#/properties/data/properties/transaction/properties/transfers/items/properties/kind",
                    "type": "string",
                    "title": "The Kind Schema",
                    "default": "",
                    "examples": [
                      "transfer"
                    ],
                    "pattern": ""
                  }
