            "to",
            "nonce",
            "value",
            "gas_price",
            "gas_used",
            "transaction_fee",
            "block_confirmation",
            "status",
            "updated_timestamp",
            "block_timestamp",
            "block_number",
            "rule_name",
            "meta_property",
            "transfers"
          ],
          "properties": {
            "id": {
              "$id": "#/properties/data/properties/transaction/properties/id",
              "type": "string",
              "title": "The Id Schema",
              "default": "",
              "examples": [
                "6460d2d2-a7c6-4365-bdf4-4d24bd9c217e"
              ],
              "pattern": ""
            },
            "transaction_hash": {
              "$id": "#/properties/data/properties/transaction/properties/transaction_hash",
