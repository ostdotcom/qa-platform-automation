            "updated_timestamp": {
              "$id": "#/properties/data/properties/transaction/properties/updated_timestamp",
              "type": "integer",
              "title": "The Updated_timestamp Schema",
              "default": 0,
              "examples": [
                1555333579
              ]
            },
            "block_timestamp": {
              "$id": "#/properties/data/properties/transaction/properties/block_timestamp",
              "type": "null",
              "title": "The Block_timestamp Schema",
              "default": null,
              "examples": [
                null
              ]
